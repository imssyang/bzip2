cp -f libbz2.a /opt/bzip2/lib
chmod a+r /opt/bzip2/lib/libbz2.a
cp -f bzgrep /opt/bzip2/bin/bzgrep
ln -s -f /opt/bzip2/bin/bzgrep /opt/bzip2/bin/bzegrep
ln -s -f /opt/bzip2/bin/bzgrep /opt/bzip2/bin/bzfgrep
chmod a+x /opt/bzip2/bin/bzgrep
cp -f bzmore /opt/bzip2/bin/bzmore
ln -s -f /opt/bzip2/bin/bzmore /opt/bzip2/bin/bzless
chmod a+x /opt/bzip2/bin/bzmore
cp -f bzdiff /opt/bzip2/bin/bzdiff
ln -s -f /opt/bzip2/bin/bzdiff /opt/bzip2/bin/bzcmp
