cp -f bzgrep.1 bzmore.1 bzdiff.1 /opt/bzip2/man/man1
chmod a+r /opt/bzip2/man/man1/bzgrep.1
chmod a+r /opt/bzip2/man/man1/bzmore.1
chmod a+r /opt/bzip2/man/man1/bzdiff.1
echo ".so man1/bzgrep.1" > /opt/bzip2/man/man1/bzegrep.1
echo ".so man1/bzgrep.1" > /opt/bzip2/man/man1/bzfgrep.1
