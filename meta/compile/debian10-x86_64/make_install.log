❯ make install PREFIX=/opt/bzip2 2>../make_install 1>&2
if ( test ! -d /opt/bzip2/bin ) ; then mkdir -p /opt/bzip2/bin ; fi
if ( test ! -d /opt/bzip2/lib ) ; then mkdir -p /opt/bzip2/lib ; fi
if ( test ! -d /opt/bzip2/man ) ; then mkdir -p /opt/bzip2/man ; fi
if ( test ! -d /opt/bzip2/man/man1 ) ; then mkdir -p /opt/bzip2/man/man1 ; fi
if ( test ! -d /opt/bzip2/include ) ; then mkdir -p /opt/bzip2/include ; fi
cp -f bzip2 /opt/bzip2/bin/bzip2
cp -f bzip2 /opt/bzip2/bin/bunzip2
cp -f bzip2 /opt/bzip2/bin/bzcat
cp -f bzip2recover /opt/bzip2/bin/bzip2recover
chmod a+x /opt/bzip2/bin/bzip2
chmod a+x /opt/bzip2/bin/bunzip2
chmod a+x /opt/bzip2/bin/bzcat
chmod a+x /opt/bzip2/bin/bzip2recover
cp -f bzip2.1 /opt/bzip2/man/man1
